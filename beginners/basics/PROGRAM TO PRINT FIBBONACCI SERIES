#include <stdio.h>
int fib(int arr[],int n);
int main()
{
    int n;
    printf("enter the number to enter fibbonacci series should be greater than 1");
    scanf("%d", &n);
    int arr[n];
    fib(arr,n);
    return 0;
}
int fib(int arr[], int n){
    int fib[n];
    fib[0]= 0;
    fib[1]= 1;
    for(int i=2;i<n;i++){
        fib[i]=fib[i-1]+fib[i-2];
        printf("%d\t", fib[i]);
    }

    
}
